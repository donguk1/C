#include <stdio.h>

void main()
{
	char stack[5];
	int top = 0;

	stack[tap] = 'A';
	printf("%c 자도차가 터널에 들어감\n", stack[top]);
	top++;

	stack[tap] = 'B';
	printf("%c 자도차가 터널에 들어감\n", stack[top]);
	top++;

	stack[tap] = 'C';
	printf("%c 자도차가 터널에 들어감\n", stack[top]);
	top++;

	printf("\n");

	top--;
	printf("%c 자동차가 터널을 빠져나감\n", stacck[top]);
	stack[top] = ' ';

	top--;
	printf("%c 자동차가 터널을 빠져나감\n", stacck[top]);
	stack[top] = ' ';

	top--;
	printf("%c 자동차가 터널을 빠져나감\n", stacck[top]);
	stack[top] = ' ';
}
